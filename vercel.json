{
  "version": 2,
  "builds": [
    {
      "src": "client/live-poll-frontend/next.config.js",
      "use": "@vercel/next"
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/api/$1"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/server/api/socket"
    },
    {
      "src": "/(.*)",
      "dest": "/client/live-poll-frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SOCKET_URL": "",
    "NODE_ENV": "production"
  }
}